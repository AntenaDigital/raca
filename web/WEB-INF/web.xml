<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>RacaMasterAckProxy</servlet-name>
    <servlet-class>raca.server.RacaMasterAckProxy</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>RacaMasterAckProxy</servlet-name>
    <url-pattern>/racamasterackproxy</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RacaMasterQueueProxy</servlet-name>
    <servlet-class>raca.server.RacaMasterQueueProxy</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>RacaMasterQueueProxy</servlet-name>
    <url-pattern>/racamasterqueueproxy</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RacaMediatorProxy</servlet-name>
    <servlet-class>raca.server.RacaMediatorProxy</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>RacaMediatorProxy</servlet-name>
    <url-pattern>/racamediatorproxy</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RacaMasterPublishProxy</servlet-name>
    <servlet-class>raca.server.RacaMasterPublishProxy</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>RacaMasterPublishProxy</servlet-name>
    <url-pattern>/racamasterpublishproxy</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RacaSendProxy</servlet-name>
    <servlet-class>raca.server.RacaSendProxy</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>RacaSendProxy</servlet-name>
    <url-pattern>/racasendproxy</url-pattern>
  </servlet-mapping>
</web-app>